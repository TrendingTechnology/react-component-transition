{
  "compilerOptions": {
    "sourceMap": false,
    "noImplicitAny": true,
    "strictBindCallApply": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "baseUrl": ".",
    "outDir": "./build",
    "jsx": "react",
    "esModuleInterop": true,
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es6"
    ]
  },
  "include": [
    "./src"
  ]
}